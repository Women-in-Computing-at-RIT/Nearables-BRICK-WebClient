    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-selector-matches": "^2.0.5",
    "postcss-selector-not": "^2.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      [
        "transform-regenerator",
        {
          "async": false
        }
      ],
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "client.test/**/*.js",
      "!client.test/setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./client.test/setup"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "eslint": "eslint client client.test run.js webpack.config.js",
    "stylelint": "stylelint \"client/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "ava --color --verbose",
    "test:watch": "ava --color --verbose --watch",
    "build": "node ./node_modules/webpack/bin/webpack --progress --colors",
    "build:release": "set RELEASE=1 && node ./node_modules/webpack/bin/webpack --progress --colors --env.release",
    "deploy": "npm run build:release && firebase deploy",
    "start": "node dev",
    "postcheckout": "echo Checking NPM packages for this branch && npm run postmerge",
    "postmerge": "echo Checking for NPM package changes && node ./node_modules/npm-install-changed/index.js"
  }
}
